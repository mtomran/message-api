{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "camelcase": [2, {
                "properties": "never"
            }
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            0,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals":{
        "config": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}